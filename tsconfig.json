{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@libraries/*": ["src/libraries/*"],
      "@layouts/*": ["src/layouts/*"],
      "@root/*": ["src/*"]
    }
  }
}